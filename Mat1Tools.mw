<Group hide-input="false" labelreference="L1" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Mat1Tools := module()
description &quot;Tools used in the DTU course 01005 - Advanced Engineering Mathematics 1&quot;;
option package;
export 
   dot, prik, cross, kryds, vop, intervalsolve, paraplot, grad, srhs, slhs, checkGradient;

with(LinearAlgebra):
with(plots):
with(Integrator8):

dot := proc(x::Vector,y::Vector);
VectorCalculus[DotProduct](x,y); 
end proc:
prik:= dot:

cross := proc(x::Vector, y::Vector);
convert(VectorCalculus[CrossProduct](x,y),Vector);
end proc:
kryds := cross:

vop := proc(v) op(convert(v,list)) end proc:

intervalsolve := Student[Calculus1][Roots]:

paraplot := proc(r::{procedure},range::{list(range)})
local var,i; var:=op(1,eval(r)):
if (numelems(range)=1) then
   if (numelems(r(var))=2) then
      plot([vop(r(var)),var=range[1]],_rest); # Kurve i 2D
   else
      plot3d(r(var),var=range[1],_rest) # Kurve i 3D
   end if:
elif (numelems(range)=2) then
   if (numelems(r(var))=2) then
      plot3d(&lt;r(var),0&gt;,var[1]=range[1],var[2]=range[2],orientation=[-90,0],lightmodel=none,_rest); # Plan i 2D
   else
      plot3d(r(var),var[1]=range[1],var[2]=range[2],_rest); # Flade i 3D
   end if:
elif (numelems([var])=3) then
   plots[display](Integrator8[sideFlader](r,[seq(vop(convert(range[i],list)),i=1..3)],[8,8,8]),_rest);
end if;
end proc:

grad:=(X,Y)-&gt;convert(linalg[grad](X,Y),Vector[column]):

srhs := proc(X, type:=whattype(X)) 
description &quot;takes a list, vector, column matrix, or similar containing only equations and returns the right-hand side of the equations.
I.e. srhs(&lt;x=0,y=10,z=15&gt;)
&lt;0,10,15&gt;&quot;:
local i, l:
l := convert(X, list):
return convert([seq(rhs(l[i]),i=1..numelems(l))],type);
end proc:

slhs := proc(X, type:=whattype(X)) 
description &quot;takes a list, vector, column matrix, or similar containing only equations and returns the left-hand side of the equations.
I.e. slhs(&lt;x=0,y=10,z=15&gt;)
&lt;x,y,z&gt;&quot;:
local i, l:
l := convert(X, list):
return convert([seq(lhs(l[i]),i=1..numelems(l))],type);
end proc:

checkGradient := proc(V, $)
description &quot;takes a vector field in 2 or 3 dimensions as a list, vector, or similar and checks if it lives up to the mandatory criteria for being a gradient field
I.e. checkGradient(&lt;y*cos(x*y), z+x*cos(x*y), y&gt;)
Condition satisfied.&quot;:
if numelems(V) &gt; 3 then
 return &quot;The vectorfield must have a dimension of at most 3.&quot;;
elif numelems(V) &lt; 2 then
 return &quot;The vectorfield must have a dimension of at least 2.&quot;;
elif numelems(V) = 2 then
 if simplify(diff(V[1],y)) &lt;&gt; simplify(diff(V[2],x)) 
  then print(&quot;Not gradient field. &quot;, Diff(V__1,y) &lt;&gt; Diff(V__2,x))
 else print(&quot;Condition satisfied.&quot;)
 end if;
else
 if simplify(diff(V[1],y)) &lt;&gt; simplify(diff(V[2],x)) 
  then print(&quot;Not gradient field. &quot;, Diff(V__1,y) &lt;&gt; Diff(V__2,x))
 elif simplify(diff(V[1],z)) &lt;&gt; simplify(diff(V[3],x)) 
  then print(&quot;Not gradient field. &quot;, Diff(V__1,z) &lt;&gt; Diff(V__3,x))
 elif simplify(diff(V[2],z)) &lt;&gt; simplify(diff(V[3],y)) 
  then print(&quot;Not gradient field. &quot;, Diff(V__2,z) &lt;&gt; Diff(V__3,y))
 else print(&quot;Condition satisfied.&quot;)
 end if;
end if;
end proc:

end module;</Text-field>
